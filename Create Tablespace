  CREATE TABLESPACE Yr2013 DATAFILE '+DATA' SIZE 500M
    AUTOEXTEND ON NEXT 20M MAXSIZE UNLIMITED LOGGING
 EXTENT MANAGEMENT LOCAL SEGMENT SPACE MANAGEMENT AUTO;
 
